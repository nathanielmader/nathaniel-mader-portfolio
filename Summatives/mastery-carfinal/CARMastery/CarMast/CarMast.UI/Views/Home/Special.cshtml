@model IEnumerable<CarMast.Models.DataModels.Special>

@{
    ViewBag.Title = "Special";
}

<!--Page Seems to Work-->
<h2>Specials</h2>
<div class="row">
    @foreach (var special in Model)
    {
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-body">
                    <div class="col-md-3">
                        <!--Special Image -->
                    </div>
                    <div class="col-md-9">
                        <div class="row">
                            <!--Title-->
                            <h2>@special.SpecialTitle</h2>
                        </div>
                        <div class="row">
                            <!--Description-->
                            <p>@special.SpecialDescription</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }
</div>

